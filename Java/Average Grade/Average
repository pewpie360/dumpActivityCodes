package main;

public class Average{
	
	public static double Prelim(double prelim) {
			return prelim;
	}
	
	public static double Midterm(double x) {
		return x;
   }
	
	public static double Prefinal(double x) {
		return x;
		
	}
	
	public static double Finals(double x) {
		return x;
		
	}
	
    public static double Avg(double prelim, double mid, double finals, double prefinal) {
    	
    	double avg = (prelim + mid + prefinal + finals) / 4;
		if(avg > 3.25) {
			
			System.out.println("Fail");
		}else {
			
			System.out.println("Pass");
		}
		return avg;
    }
	}
