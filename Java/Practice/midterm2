package learningJava;
import java.util.*;

public class Main {
	
	public static void main(String[] args) {
		
		String[] name = {"Joe", "Sue", "Tom", "Jill", "Patty", "Meow", "York"};
		
		for(int i=0; i<name.length; i=i+2) {
			
			/*
			 * Joe[0] 
			 * Tom[2]
			 * Patty[4]
			 * York[6]
			 */
			System.out.println(name[i]+ " ");
		}
	}
}
