package main;

import java.util.Scanner;

public class Main {
	
	public static void main(String args[])
	  {
		
		Scanner sc = new Scanner(System.in);
		
		Students myObj = new Students();
		
		System.out.println("Enter Name: ");
		String name = sc.nextLine();
		
		
		int[] grade = new int[5];
		
		System.out.println("Enter five subject");
		for(int i = 0; i < grade.length; i++ )
		{
			double input1 = sc.nextInt();
			String input2 = sc.nextLine();
			myObj.setAverage(input1);
			//Ga return null 
			myObj.setInformation(input2, input2, input2, input2, input2);
		}
		
		//Output
		System.out.println(myObj.StudentName(name));
		System.out.println(myObj.getAverage());
		System.out.println(myObj.getInformation());
		System.out.println(myObj.getQualifications());
		
	  }
}
