package main;

public class Students{
	
	private static int avg;
	
	public static String Name(String name) 
	{	
		return name;
	}
	
	public static int StudentID(int id) 
	{
		return id;
	}
	
	public static int CalculateAverage(int sub1, int sub2, int sub3, int sub4, int sub5) 
	{
		int sum = sub1 + sub2 + sub3 + sub4 + sub5;
		avg = (int) (sum / 5);
		
		System.out.println("\nSubject #1: \n" + sub1 + "\nSubject #2:\n"+ sub2 + "\nSubject #3:\n" + sub3 + "\nSubject #4: \n"+ sub4 + "\nSubject #5:\n"+ sub5);
		return avg;
	}
	
	public static int getQualifications(int sub1, int sub2, int sub3, int sub4, int sub5) {
		
		int sum = sub1 + sub2 + sub3 + sub4 + sub5;
		avg = (int) (sum / 5);
		
		if(avg <= 75) {
			
			System.out.println("Damn u fail this time");
			
			
		}else {
			
			System.out.println("Congrats, Proceed to the next level");
		}
		return 0;
	}
}

 
