package main;

public class Students{
	
	private double sum = 0;
	private double avg  = 0;
	private String input;
	
	//Calculate Average: Using getters and setters method
	public double getAverage() 
	{
		System.out.print("Average Grade:");
		return avg;
	}
	
	public void setAverage(double x) 
	{
		sum += x;
		avg = sum  / 5;
	}
	
	public String StudentName(String name)
	{		
		return "\nHello " + name;
	}
	
	//ga return Null
	public String getInformation() 
	{
		return input;
	}
	
	//ga Return Null
	public void setInformation(String a, String b, String c, String d, String e) {
	   
		input = a + b + c + d + e;
	}
	
	public double getQualifications()
	{
		
		if(avg > 75) 
		{
			System.out.println("Congratz m8, Proceed to the next level");
		}
		
		else 
		{
			System.out.println("Damn you fail this time m8");
		}
		return 0;
	}
}
