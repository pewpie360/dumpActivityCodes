package main;

public class Calculator {
	
	
	public static String Person(String name) {
		return "Hello " + name;	
	}
	
	public static int addCalculator(int result, int x, int y) {
		
		result = x + y;
		return result;
		
	}
	
	public static int subtractCalculator(int result, int x, int y) {
		
		result = x - y;
		return result;
		
	}
	
	public static int multiplyCalculator(int result, int x, int y) {
		
		result = x * y;
		return result;
		
	}
	
	public static int divideCalculator(int result, int x, int y) {
		
		result = x / y;
		return result;
		
	}
	
	public static int remainderCalculator(int result, int x, int y) {
		
		result = x % y;
		return result;
		
	}
}
